{
  "name": "@takram/planck-geography",
  "version": "0.1.9",
  "description": "",
  "repository": "takram-design-engineering/planck-geography",
  "main": "dist/planck-geography.js",
  "browser": "dist/planck-geography.js",
  "module": "dist/planck-geography.module.js",
  "author": "Shota Matsuda",
  "license": "MIT",
  "homepage": "http://takram.com/projects/planck",
  "files": [
    "package.json",
    "README.md",
    "dist/planck-geography.js",
    "dist/planck-geography.min.js",
    "dist/planck-geography.module.js",
    "data",
    "src"
  ],
  "scripts": {
    "build": "npm-run-all main:compile main:compress",
    "main:compile": "rollup -c",
    "main:compress": "uglifyjs --compress --mangle --output ${npm_package_main%.*}.min.js ${npm_package_main}",
    "test": "npm-run-all lint test:compile test:node test:cloud",
    "test:compile": "npm-run-all --parallel test:compile:rollup test:compile:webpack",
    "test:compile:rollup": "rollup -c rollup.config.unit.js",
    "test:compile:webpack": "webpack --config webpack.config.unit.js",
    "test:node": "mocha --recursive test/unit",
    "test:browser": "browser-sync start --server . --files dist --startPath test --no-open --no-notify",
    "test:cloud": "mocha test/setup.js",
    "lint": "npm-run-all lint:main lint:test",
    "lint:main": "standard -v *.js src/*.js src/**/*.js",
    "lint:test": "standard -v --env mocha test/*.js test/**/*.js",
    "lint-fix": "npm-run-all lint-fix:main lint-fix:test",
    "lint-fix:main": "standard -v --fix *.js src/*.js src/**/*.js",
    "lint-fix:test": "standard -v --fix --env mocha test/*.js test/**/*.js",
    "prewatch": "npm-run-all main:compile test:compile",
    "watch": "npm-run-all --parallel test:node watch:main watch:test watch:rollup watch:webpack test:browser",
    "watch:main": "nodemon -C --watch rollup.config.js --watch src --exec 'npm-run-all main:compile test:node'",
    "watch:test": "nodemon -C --watch test --exec 'npm-run-all test:compile test:node'",
    "watch:rollup": "nodemon -C --watch rollup.config.unit.js --exec 'npm run test:compile:rollup'",
    "watch:webpack": "nodemon -C --watch webpack.config.unit.js --exec 'npm run test:compile:webpack'",
    "data-setup": "npm-run-all --parallel data-setup:japan data-setup:us data-setup:world",
    "data-setup:japan": "node data/index.js setup --preset=japan --output=dist/data --identifier=japan --levels=prefecture,municipality --method=topojson --encoding=shift-jis",
    "data-setup:us": "node data/index.js setup --preset=us --output=dist/data --identifier=us --levels=state,county --method=mapshaper --encoding=utf-8",
    "data-setup:world": "node data/index.js setup --preset=world --output=dist/data --identifier=world --levels=country --method=topojson --encoding=utf-8",
    "data-project": "npm-run-all --parallel data-project:japan data-project:us data-project:world",
    "data-project:japan": "node data/index.js project --output=dist/data --identifier=japan --levels=prefecture,municipality --name=TransverseMercator --origin=[137.2,37] --rotates=true",
    "data-project:us": "node data/index.js project --output=dist/data --identifier=us --levels=state,county --name=AlbersUsa --origin=[-99.5,39.5] --rotates=false",
    "data-project:world": "node data/index.js project --output=dist/data --identifier=world --levels=country --name=PeirceQuincuncial --origin=[45,90] --rotates=true"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.4.0",
    "decompress": "^4.2.0",
    "download": "^6.2.5",
    "earcut": "^2.1.3",
    "mapshaper": "^0.4.65",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "polylabel": "^1.0.2",
    "shapefile": "^0.6.6",
    "suncalc": "^1.8.0",
    "svg-path-parser": "^1.1.0",
    "topojson": "^3.0.2"
  },
  "peerDependencies": {
    "@takram/planck-core": "0.x",
    "d3": "^5.x",
    "d3-geo-projection": "^2.x",
    "three": "^0.92.0"
  },
  "devDependencies": {
    "@takram/planck-core": "^0.6.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "browser-sync": "^2.23.7",
    "chai": "^4.1.2",
    "d3": "^5.1.0",
    "d3-geo-projection": "^2.4.0",
    "eslint": "^4.19.1",
    "express": "^4.16.3",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.1.2",
    "rollup": "^0.58.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "sauce-connect-launcher": "^1.2.4",
    "saucelabs": "^1.4.0",
    "source-map-support": "^0.5.4",
    "standard": "^11.0.1",
    "three": "^0.92.0",
    "uglify-js": "^3.3.22",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15"
  },
  "saucelabs": {
    "framework": "mocha",
    "platforms": [
      [
        "Windows 10",
